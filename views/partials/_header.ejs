<header>
  <h1>TinyApp</h1>
  <h3>A site for shortening long URLs...</h3>

  <% if(user) { %>

  <p>Welcome, <%= user.email %>!</p>

  <form action="/logout" method="POST">
  <input type="submit" value="Logout">
  </form>

  <% } else { %>

  <a href="/register">Register</a>
  <a href="/login">Login</a>

  <% } %>
</header>